{
  "env": {
    "test": {
      "plugins": [
        "transform-vue-jsx",
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ],
      "presets": [
        "stage-1"
      ]
    },
    "development": {
      "plugins": [
        "transform-vue-jsx"
      ],
      "presets": [
        ["es2015", {"modules": false}],
        "stage-1"
      ]
    },
    "production": {
      "plugins": [
        "transform-vue-jsx",
        "transform-runtime",
        ["component", [{
          "libraryName": "element-ui",
          "styleLibraryName": "theme-default"
        }]]
      ],
      "presets": [
        ["es2015", {"modules": false}],
        "stage-1"
      ]
    }
  }
}
